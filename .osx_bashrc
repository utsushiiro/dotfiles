export CLICOLOR=1

# ls
if which lsd > /dev/null; then
  alias ls='lsd'
else 
  alias ls='ls -G -F'
fi

alias l='ls -l'
alias la='ls -a'
alias lla='ls -la'
alias lt='ls --tree'

alias pwd='pwd | tee >(pbcopy)'
alias h="history | grep"

# cd
alias cdd='cd ~/Desktop'
alias cdc='cd `pbpaste`'

# Sublime
alias sl="subl"

# open
  alias finder="open ."

# jupyter
  alias note='jupyter notebook'
  alias qtc='jupyter qtconsole'